<div class="container">
	<div class="row">
		<h2>Add a collaborator: <%= @wiki.name %></h2>
		<%= form_for @wiki do |f| %>
			<%= f.select :user_id, User.all.collect { |u| [u.email, u.id] }, include_blank: true %>
			<%= hidden_field(:wiki, :id) %>
			<%= f.submit %>
		<% end %>
			

	</div>
</div>
